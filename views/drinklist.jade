extends layout

block content
  h1.title(id="title") All Drinks
    each drink, i in drinklist
      a.plainlink(href="/drinks/find/#{drink.name}")
        h3= drink.name

block scripts
  script(src='/javascripts/jquery-2.1.3.min.js')
  script(src='/javascripts/jquery-ui-1.11.4/jquery-ui.min.js')
  script(src='/javascripts/global.js')